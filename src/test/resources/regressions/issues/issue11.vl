struct cell {
  int f;
}

procedure test01(cell c)
  requires c.f |-> _;
  ensures  c.f |-> _;
{
  assert c != null && c.f |-> _ && c != null;
  exhale (c != null && c.f |-> _) && c != null;
  inhale c != null && (c.f |-> _ && c != null);
}

predicate P(cell c) { c.f |-> _ }

