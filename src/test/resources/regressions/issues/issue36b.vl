struct hd {
  node ptr;
}

struct node {
  int val;
  node next;
}


region Qu(id s, hd h)
  guards {
    duplicable G;
  }
  interpretation {
    h.ptr |-> ?x
  }
  state { 0 }
  actions {}



procedure makeQueue(id r) returns (hd ret)
  requires true;
  ensures Qu(r, ret);
{
  node y;
  hd x;

  inhale (y.val |-> ?v);
  inhale (y.next |-> ?n);

  y.next := null;

  inhale (x.ptr |-> ?p);

  x.ptr := y;

  ret := x;
  fold Qu(r, ret);
}
