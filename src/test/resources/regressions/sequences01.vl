procedure test01(seq<int> xs) returns (int ret)
  requires size(xs) > 0;
  ensures  ret == head(xs);
{
  seq<int> ys;

  ys := tail(xs);
  ret := head(xs);

  assert size(ys) == size(xs) - 1;
}

procedure test02() returns (int ret)
  ensures ret == 1;
{
  seq<int> xs;
  int x;

  xs := Seq(1, 2, 3);
  x := test01(xs);

  ret := x;
}
