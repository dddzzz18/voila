procedure test01() {
  pair<int, bool> p;

  p := Pair[int, bool](1, true);
  assert fst(p) == 1;

  assert fst(Pair(1, true)) == 1;
  assert snd(Pair(1, true)) == true;
}

procedure test02(pair<bool, int> p1, pair<bool, int> p2) {
  assume p1 == p2;
  assert fst(p1) == fst(p2) && snd(p1) == snd(p2);
}

procedure test03(pair<bool, int> p1, pair<bool, int> p2) {
  assume fst(p1) == fst(p2) && snd(p1) == snd(p2);
  assert p1 == p2;
}

procedure test04(int x1, frac x2, int y1, frac y2) {
  assume Pair(x1, x2) == Pair(y1, y2);
  assert x1 == y1 && x2 == y2;
}