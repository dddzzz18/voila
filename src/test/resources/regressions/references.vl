struct llnode {
  int val;
  llnode nxt;
}

procedure test() {
  llnode n;

  inhale n.val |-> ?v1;
  inhale n.nxt |-> ?v2;

  n.val := 0;
  n.nxt := null;
}

region R(id a, llnode x)
  guards { unique G; }
  interpretation { x.nxt |-> ?n }
  state { n }
  actions { G: null ~> null; }
